      <object class="GtkImageMenuItem" id="mnuDuplicate">
        <property name="label" translatable="yes">Duplicate</property>
        <property name="visible">True</property>
        <property name="can_focus">False</property>
        <property name="tooltip_text" translatable="yes">Make a copy of this clip</property>
        <property name="use_action_appearance">False</property>
        <property name="image">gtk-copy</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <signal name="activate" handler="on_mnuDuplicate_activate" swapped="no"/>
      </object>
    </child>
    <child>
      <object class="GtkSeparatorMenuItem" id="menuitem5a">
        <property name="visible">True</property>
        <property name="can_focus">False</property>
