<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Flippo</title>
	<description translatable="True">Invert horizontally or vertically</description>
	<icon>flippo.png</icon>
	<category>Video</category>
	<service>frei0r.flippo</service>
	<param name="X axis" type="dropdown" title="X axis" description="Invert on the X axis">
		<values>
			<value name="Yes" num="1.0"/>
			<value name="No" num="0.0"/> 
		</values>
		<default>1.0</default>
	</param>
	<param name="Y axis" type="dropdown" title="Y axis" description="Invert on the Y axis">
		<values>
			<value name="Yes" num="1.0"/>
			<value name="No" num="0.0"/> 
		</values>
		<default>1.0</default>
	</param>

</effect>
