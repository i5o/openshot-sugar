<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Threshold</title>
	<description translatable="True">Reduces and mixes the colors of the image based on a threshold</description>
	<icon>threshold.png</icon>
	<category>Video</category>
	<service>frei0r.threshold0r</service>
	
	<param name="Threshold" type="hscale" title="Threshold" description="">
		<min>0.0</min>
		<max>2.0</max>
		<step>0.01</step>
		<default>0.5</default>
	</param>

</effect>
