<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Distort</title>
	<description translatable="True">Distort the source similar to plasma</description>
	<icon>distort.png</icon>
	<category>Video</category>
	<service>frei0r.distort0r</service>
	
	<param name="Amplitude" type="spinner" title="Amplitude" description="">
		<min>0.0</min>
		<max>1.0</max>
		<step>0.01</step>
		<default>0.1</default>
	</param>
	
	<param name="Frequency" type="spinner" title="Frequency" description="">
		<min>0.0</min>
		<max>1.0</max>
		<step>0.01</step>
		<default>0.3</default>
	</param>
</effect>
