<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy toplevel-contextual -->
  <object class="GtkMenu" id="mnuTrackPopup">
    <property name="visible">True</property>
    <property name="events">GDK_POINTER_MOTION_MASK | GDK_POINTER_MOTION_HINT_MASK | GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
    <child>
      <object class="GtkImageMenuItem" id="mnuAddTrackAbove">
        <property name="label" translatable="yes">Add Track Above</property>
        <property name="visible">True</property>
        <property name="events">GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
        <property name="tooltip_text" translatable="yes">Add Track Above</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <property name="image">gtk-add1</property>
        <signal name="activate" handler="on_mnuAddTrackAbove_activate"/>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="mnuAddTrackBelow">
        <property name="label" translatable="yes">Add Track Below</property>
        <property name="visible">True</property>
        <property name="events">GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
        <property name="tooltip_text" translatable="yes">Add Track Below</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <property name="image">gtk-add2</property>
        <signal name="activate" handler="on_mnuAddTrackBelow_activate"/>
      </object>
    </child>
    <child>
      <object class="GtkSeparatorMenuItem" id="menuitem3">
        <property name="visible">True</property>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="mnuMoveTrackUp">
        <property name="label" translatable="yes">Move Track Up</property>
        <property name="visible">True</property>
        <property name="events">GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
        <property name="tooltip_text" translatable="yes">Move Track Up</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <property name="image">gtk-go-up</property>
        <signal name="activate" handler="on_mnuMoveTrackUp_activate"/>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="mnuMoveTrackDown">
        <property name="label" translatable="yes">Move Track Down</property>
        <property name="visible">True</property>
        <property name="events">GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
        <property name="tooltip_text" translatable="yes">Move Track Down</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <property name="image">gtk-go-down</property>
        <signal name="activate" handler="on_mnuMoveTrackDown_activate"/>
      </object>
    </child>
    <child>
      <object class="GtkSeparatorMenuItem" id="menuitem8">
        <property name="visible">True</property>
      </object>
    </child>   
    <child>
      <object class="GtkImageMenuItem" id="mnuRenameTrack">
        <property name="label" translatable="yes">Rename Track</property>
        <property name="visible">True</property>
        <property name="events">GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
        <property name="tooltip_text" translatable="yes">Rename Track</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <property name="image">gtk-edit</property>
        <signal name="activate" handler="on_mnuRenameTrack_activate"/>
      </object>
    </child>
    <child>
      <object class="GtkSeparatorMenuItem" id="menuitem4">
        <property name="visible">True</property>
      </object>
    </child>
    <child>
      <object class="GtkImageMenuItem" id="mnuClearTrack">
        <property name="label" translatable="yes">Clear Track</property>
        <property name="visible">True</property>
        <property name="events">GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
        <property name="tooltip_text" translatable="yes">Clear Track</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <property name="image">gtk-clear</property>
        <signal name="activate" handler="on_mnuClearTrack_activate"/>
      </object>
    </child> 
    <child>
      <object class="GtkImageMenuItem" id="mnuRemoveTrac">
        <property name="label" translatable="yes">Remove Track</property>
        <property name="visible">True</property>
        <property name="events">GDK_BUTTON_PRESS_MASK | GDK_BUTTON_RELEASE_MASK | GDK_STRUCTURE_MASK</property>
        <property name="tooltip_text" translatable="yes">Remove Track</property>
        <property name="use_stock">False</property>
        <property name="accel_group">accelgroup1</property>
        <signal name="activate" handler="on_mnuRemoveTrack_activate"/>
        <property name="image">gtk-remove</property>
      </object>
    </child>
  </object>
  <object class="GtkAccelGroup" id="accelgroup1"/>
  
  <object class="GtkImage" id="gtk-go-up">
    <property name="visible">True</property>
    <property name="stock">gtk-go-up</property>
  </object>
  <object class="GtkImage" id="gtk-go-down">
    <property name="visible">True</property>
    <property name="stock">gtk-go-down</property>
  </object>
  <object class="GtkImage" id="gtk-add1">
    <property name="visible">True</property>
    <property name="stock">gtk-add</property>
  </object>
  <object class="GtkImage" id="gtk-add2">
    <property name="visible">True</property>
    <property name="stock">gtk-add</property>
  </object>
  <object class="GtkImage" id="gtk-edit">
    <property name="visible">True</property>
    <property name="stock">gtk-edit</property>
  </object>
  <object class="GtkImage" id="gtk-clear">
    <property name="visible">True</property>
    <property name="stock">gtk-clear</property>
  </object>
  <object class="GtkImage" id="gtk-remove">
    <property name="visible">True</property>
    <property name="stock">gtk-remove</property>
  </object>
</interface>
