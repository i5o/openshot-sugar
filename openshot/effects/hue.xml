<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE openshot-effect>
<effect>
	<title translatable="True">Hue</title>
	<description translatable="True">Shift the hue of all colors in the image</description>
	<icon>hue.png</icon>
	<category>Video</category>
	<service>frei0r.hueshift0r</service>
	
	<param name="Hue" type="hscale" title="Hue" description="">
		<min>0.0</min>
		<max>1.0</max>
		<step>0.01</step>
		<default>0.0</default>
	</param>

</effect>

